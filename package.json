	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"sync": "svelte-kit sync && rm -rf 'src/db/models' && npx prisma generate && prettier --plugin-search-dir . --write 'src/db/models/**/*'",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"ts": "tsc --pretty --noImplicitAny --noEmit",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,svelte}": [
			"prettier --plugin-search-dir  --write",
			"eslint --fix"
		],
		"*.{html,md,mdx,json,css}": "prettier --plugin-search-dir  --write"
	},
