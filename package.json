	},
	"scripts": {
		"build": "vite build",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-check --tsconfig ./tsconfig.json --output human-verbose",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint:eslint": "TIMING=1 eslint . --cache",
		"lint:prettier": "prettier --check .",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json --output machine",
		"lint": "yarn format && yarn lint:eslint",
		"prepare": "husky install",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"ts": "tsc --pretty --noImplicitAny --noEmit"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,svelte}": [
			"prettier --plugin-search-dir  --write",
			"eslint --fix"
		],
		"*.{html,md,mdx,json,css}": "prettier --plugin-search-dir  --write"
	},
