		"prettier-plugin-tailwindcss": "^0.4.1",
		"prisma": "^4.15.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-meta-tags": "^2.8.0",
		"svelte-preprocess": "^5.0.4",
		"sveltekit-superforms": "^0.8.7",
		"tailwind-merge": "^1.13.2",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
